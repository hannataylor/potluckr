<h1>Login</h1>

<div>
<%= form_for :session, url: {action: "create"} do |f| %>

  <h4><%= f.label :username %></h4>
    <%= f.text_field :username %>

  <h4><%= f.label :password %></h4>
    <%= f.password_field :password %>

  <% if @errors %>
      <h5><%= @errors.first %></h5>
  <% end %>


    <%= f.submit "Login"%>

<% end %>
</div>