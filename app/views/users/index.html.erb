<div class="container">
  <div class="jumbotron">
    <h1 class="uppercase-spaced">Users</h1>

<table class="table table-condensed">

  <% @users.each do |user| %>
  <tr>
    <td><%= link_to "#{user.first_name} #{user.last_name}", user_path(user) %></td>
  </tr>
  <% end %>
</table>
  </div>
</div>


