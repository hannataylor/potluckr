<h1>Create a Potluck</h1>

<%= form_for(@event) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :location %>
  <%= f.text_field :location %>

  <%= f.label :description %>
  <%= f.text_area :description %>

  <%= f.label :date %>
  <%= f.date_field :date%>
  <%= f.submit "Create Potluck"%>
<% end %>
